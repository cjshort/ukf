 <%= form_for(@post) do |post| %>
	 <%= post.label :image %>
   <%= post.text_field :image %>
   <%= post.label :title %>
   <%= post.text_field :title %>
	 <%= post.label :content %>
   <%= post.text_area :content %>
	 <%= post.label :name %>
   <%= post.text_field :name %>
	 <%= post.label :website %>
   <%= post.text_field :website %>
	 <%= post.label :approved %>
   <%= post.text_field :approved %>
   <%= post.submit %>
 <% end %>